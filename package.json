{
  "name": "@xilution/todd-coin-constants",
  "version": "1.1.0",
  "description": "The Todd Coin Cryptocurrency Constants",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./coverage ./dist",
    "format": "prettier --write .",
    "watch": "nodemon --watch src -e ts --exec \"npm run build\"",
    "verify": "prettier --check . && eslint . --ext .js,.ts && npm run test && npm run build",
    "semantic-release": "semantic-release",
    "test": "jest ./test",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xilution/todd-coin-constants"
  },
  "bugs": {
    "url": "https://github.com/xilution/todd-coin-constants/issues"
  },
  "homepage": "https://github.com/xilution/todd-coin-constants",
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "semantic-release": "^19.0.2",
    "typescript": "^4.6.3"
  },
